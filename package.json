{
  "name": "the_kroshet_nani",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env.local -- next dev --turbopack --port 3000",
    "build": "npx prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "db:push": "dotenv -e .env.local -- npx prisma db push",
    "db:seed": "dotenv -e .env.local -- node prisma/seed.js",
    "db:reset": "dotenv -e .env.local -- npx prisma db push --force-reset && npm run db:seed",
    "db:studio": "dotenv -e .env.local -- npx prisma studio",
    "db:migrate": "dotenv -e .env.local -- npx prisma migrate dev",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.8",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^6.16.2",
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "cashfree-pg": "^5.0.8",
    "chart.js": "^4.5.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-auth": "^4.24.11",
    "nodemailer": "^6.10.1",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "19.1.0",
    "react-hot-toast": "^2.6.0",
    "uuid": "^13.0.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "dotenv-cli": "^10.0.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4"
  }
}
